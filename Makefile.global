PREFIX = /usr

MAJOR_VERSION = 1
MINOR_VERSION = 0
MICRO_VERSION = 7

VERSION = ${MAJOR_VERSION}.${MINOR_VERSION}.${MICRO_VERSION}

CC = gcc
LD = gcc
CINCLUDE =
CFLAGS = -Wall -g3 -DVERSION=\"$(VERSION)\"
